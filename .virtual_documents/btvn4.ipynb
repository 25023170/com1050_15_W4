def do_dai_day_collaz(x):
    if x < 1:
        return 0
    n=int(input())
    length = 1            
    for x in range (2,n+1):    
        if x % 2 == 0:
            x = x // 2
        else:
            x = x * 3 + 1
        length += 1
    return length




