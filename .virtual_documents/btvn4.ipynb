def dem_chu_so(n):
    n = abs(n)
    if n == 0:
        return 1
    count = 0
    while n > 0:
        count += 1
        n //= 10
    return count
n = int(input("Nhập số: "))
print("Nhập số: ", dem_chu_so(n))



